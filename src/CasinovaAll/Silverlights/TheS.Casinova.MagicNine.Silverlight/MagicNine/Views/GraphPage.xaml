<navigation:Page x:Class="TheS.Casinova.MagicNine.Views.GraphPage" 
           xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
           xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
           xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
           xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
           mc:Ignorable="d"
           xmlns:navigation="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls.Navigation"
           d:DesignWidth="640" d:DesignHeight="480"
           Title="GraphPage Page">
    <Grid x:Name="LayoutRoot">
        <Grid x:Name="topGrid" VerticalAlignment="Top" Height="42">
            <StackPanel>
                <TextBlock x:Name="messageTextBlock" Text="Message" Height="41" Margin="8,0" HorizontalAlignment="Center" VerticalAlignment="Center" TextAlignment="Center" FontSize="21.333" Width="92" />
            </StackPanel>
        </Grid>
        <Grid x:Name="contentGrid" HorizontalAlignment="Left" Width="640" Margin="0,40,0,5">
            <StackPanel Margin="8">
            	<Border Height="417" HorizontalAlignment="Right" Width="624"/>
            </StackPanel>
        </Grid>
    </Grid>
</navigation:Page>
